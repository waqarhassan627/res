@{
    ViewBag.Title = "Result";
}
@model List<projectWork.Models.QuizesDTO>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width initial-sacale=1" />
    <title>Results - Questionnaire</title>
    <link href='https://fonts.googleapis.com/css?family=Slabo+27px|Oswald:400,300,700|Poiret+One' rel='stylesheet' type='text/css'>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet">
    @Scripts.Render("~/bundles/jquery")
    @Styles.Render("~/Content/css")

    <link rel="stylesheet" type="text/css" href="~/Content/MainPage/css/style.css">
    <link rel="stylesheet" type="text/css" href="~/Content/MainPage/css/themes/flat-blue.css">
</head>
<body>
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="@Url.Content("~/Main/Home")" style="font-family: 'Poiret One', cursive; font-size: 32px;">
                    <div class="icon"></div>
                    <div class="title" style="color:white;">Questionnaire</div>
                </a>
            </div>
            <div id="navbar" class="navbar-collapse collapse " aria-expanded="true">
                <ul class="nav navbar-nav navbar-right">
                    <li class="active"><a href="@Url.Content("~/Teacher/CreateTest")">Create Test</a></li>
                    <li><a href="@Url.Content("~/Teacher/Home")">Home</a></li>
                    <li><a href="@Url.Content("~/Teacher/Logout")">Logout</a></li>
                </ul>
            </div>
            <!--/.nav-collapse -->
        </div>
    </nav>
    <div class="container" style="margin-top:80px">
        <div class="row" style="color:white; align-content:center"><h2>@ViewBag.title - Result</h2>
                        <h3>Total Marks : @Model[0].TotalMarks</h3>
        </div>
        <div class="row" style="min-height:500px">
            <div class="col-md-12 box-area">
                <table class="table table-hover">
                    <thead>
                        <tr>
                            <td>Username</td>
                            <td>isTaken</td>
                            <td>Marks Obt.</td>
                            <td>Conduct Date</td>
                        </tr>
                    </thead>
                    <tbody id="tblUsers">
                        @{
                            foreach( var q in Model)
                            {
                                <tr>
                                    <td>
                                        @q.UserName
                                    </td>
                                    <td>
                                        @q.isTaken
                                    </td>
                                    <td>
                                        @q.MarksObtained
                                    </td>
                                    <td>
                                        @q.ConductDate
                                    </td>
                                </tr>
                            }
                        }
                    </tbody>

                </table>
            </div>
        </div>
    </div>
    <footer>
        <div class="container">

            Copyright&copy; by <a>Questionnaire</a>. All rights reserved.

        </div>
    </footer>
    
    <script type="text/javascript" src="~/Scripts/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Scripts/jquery-1.9.1.js"></script>
</body>
</html>